<resources>
    <!-- Progress Bar -->
    <style name="LinearProgress" parent="Theme.AppCompat.Light">
        <item name="colorAccent">@color/accent</item>
    </style>

    <!-- Toolbar -->
    <style name="Toolbar" parent="AppTheme.NoActionBar">
        <item name="android:background">@color/primary</item>
    </style>

    <!-- Import Dialog -->
    <style name="ImportDialog" parent="ImportTheme">
        <item name="android:background">@color/white</item>
    </style>

    <style name="NotificationTitle" parent="@android:style/TextAppearance.StatusBar.EventContent.Title" />
</resources>
